{
  "env": {
    "node": true
  },
  "rules": {
    "quotes": 0,
    "no-unused-vars": [2, {"vars": "all", "args": "none"}],
    "no-underscore-dangle": 0,
    "no-multi-spaces": 0,
    "no-loop-func": 0,
    "strict": 0,
    "camelcase": 0,
    "no-redeclare": 0,
    "no-path-concat": 0,
    "key-spacing": 0
  }
}
